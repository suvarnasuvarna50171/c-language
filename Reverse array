#include <bits/stdc++.h>
using namespace std;
void printReverse(int arr[],int n,int index){
    if (index < 0) return;
    cout << arr[index] << " ";
    printReverse(arr,n,index - 1);
}

int main() {
    int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++){
    cin >> arr[i]; 
    } 
    printReverse(arr,n,n - 1);
    return 0;
}
