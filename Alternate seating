#include <stdio.h>
#include <stdlib.h>

int main() {
    int n,m;
    scanf("%d %d",&n,&m);
    int seats[m];
    for(int i=0;i<m;i++){
        scanf("%1d",&seats[i]);
    }
    int availableSeats = 0;
    if(seats[0] == 0 && (m==1|| seats[1]==0)){
        availableSeats++;
        seats[0]=1;
    }
    for(int i = 1;i<m - 1;i++){
        if(seats[i]==0 && seats[i-1]==0 && seats[i+1] == 0){
            availableSeats++;
            seats[i] = 1;
        }
    }
    if (m > 1 && seats[m-1]==0 && seats[m-2]==0){
        availableSeats++;
    }
    if(availableSeats >= n){
        printf("YES\n");
    }else{
        printf("NO\n");
    }

    return 0;
}
