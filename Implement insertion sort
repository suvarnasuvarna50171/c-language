#include <stdio.h>
#include <stdlib.h>
void insertionSort(int arr[],int n){
    for(int i = 1;i<n;i++){
        int key = arr[i];
        int j = i - 1;
        while (j >=0 && arr[j] > key){
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1]=key;
        for(int k = 0;k<n;k++){
            printf("%d",arr[k]);
            if(k < n - 1){
                printf(" ");
            }
        }
        printf("\n");
    }
}

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i = 0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    insertionSort(arr,n);
     return 0;
}
