#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    int positive = 0,negative = 0,zero = 0;
    //read the array elements and count
    for(int i = 0;i<n;i++){
        scanf("%d",&arr[i]);
        if(arr[i] > 0)
        positive++;
        else if(arr[i]<0)
        negative++;
        else zero++;
    }
    //print zero count,positive count,negative count
    printf("%d %d %d\n",zero,positive,negative);


        
    return 0;
}
