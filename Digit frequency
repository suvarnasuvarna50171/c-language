#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    int digit_count[10] = {0}; // Array to store counts of digits 0-9
    
    // Input the string
    scanf("%s", str);
    
    // Traverse the string and count digits
    for (int i = 0; i < strlen(str); i++) {
        if (str[i] >= '0' && str[i] <= '9') {
            digit_count[str[i] - '0']++;
        }
    }
    
    // Print the frequencies of each digit
    for (int i = 0; i < 10; i++) {
        printf("%d ", digit_count[i]);
    }
    printf("\n");
    
    return 0;
}
