#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    //read the array elements
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    //find the maximum element
    int max = arr[0];
    for(int i = 1;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }
    }
    //print the maximum element
    printf("%d\n",max);

   
    return 0;
}
