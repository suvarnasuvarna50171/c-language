#include <stdio.h>
#include <stdlib.h>
void binarySearch(int arr[],int n,int target){
    int low = 0,high = n - 1;
    while (low <= high){
        int mid = (low + high)/2;
        printf("%d %d %d\n",low,high,mid);
        if(arr[mid]==target){
            printf("True\n");
            return;
        }else if (arr[mid] < target){
            low = mid + 1;
        }else{
            high = mid - 1;
        }
    }
    printf("False\n");
}

int main() {
    int n,target;
    scanf("%d %d",&n,&target);
    int arr[n];
    for (int i = 0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    binarySearch(arr,n,target);

    
    return 0;
}
