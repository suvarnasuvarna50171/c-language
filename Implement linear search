#include <stdio.h>
#include <stdlib.h>
int linearSearch(int arr[],int n,int key){
    for(int i = 0;i<n;i++){
        if(arr[i] == key){
            return i;
        }
    }
    return -1;
}

int main() {
    int n,key;
    scanf("%d %d",&n,&key);
    int arr[n];
    for(int i = 0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int result = linearSearch(arr,n,key);
    printf("%d\n",result);

   
    return 0;
}
