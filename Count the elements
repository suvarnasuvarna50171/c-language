#include <stdio.h>

int main() {
    int N,M;
    scanf("%d %d",&N,&M);
    int matrix[N][M];
    for (int i = 0; i < N;i++){
        for (int j = 0;j < M;j++){
            scanf("%d",&matrix[i][j]);
        }
    }  
    int A;
    scanf("%d",&A);
    int arr[A];
    for (int i = 0;i < A;i++){
        scanf("%d",&arr[i]);
    }
    for(int i = 0;i<N;i++){
        int count = 0;
        for(int j = 0;j < M;j++){
            for(int k = 0;k < A;k++){
                if(matrix[i][j] == arr[k]){
                    count++;
                    break;
                }
            }
        }
        printf("%d\n",count);
    }
    return 0;
}
