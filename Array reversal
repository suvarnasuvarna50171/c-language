#include <stdio.h>

int main() {
    int n; // size of the array
    scanf("%d", &n);

    int arr[n];
    
    // Input the elements of the array
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Print the array in reverse order
    for (int i = n - 1; i >= 0; i--) {
        printf("%d ", arr[i]);
    }
    printf("\n"); // Print a newline at the end

    return 0;
}
