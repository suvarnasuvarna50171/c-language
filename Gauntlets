#include <stdio.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d",&N);
    int A[N];
    for(int i = 0;i<N;i++){
        scanf("%d",&A[i]);
    }
    int freq[1001]={0};
    for(int i = 0;i<N;i++){
        freq[A[i]]++;
    }
    int totalPairs = 0;
    for(int i = 0;i<=1000;i++){
        totalPairs += freq[i]/2;
    }
    printf("%d\n",totalPairs);

      
    return 0;
}
