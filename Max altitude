#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i = 0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int altitude = 0;
    int maxAltitude = 0;
    for(int i = 0;i<n;i++){
        altitude += arr[i];
        if(altitude > maxAltitude){
            maxAltitude = altitude;
        }
    }
    printf("%d\n", maxAltitude);

    
    return 0;
}
