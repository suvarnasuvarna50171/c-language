#include <stdio.h>
#include <stdlib.h>

int main() {
    int n,sum = 0;
    scanf("%d",&n);
    int matrix[n][n];
    for(int i = 0;i<n;i++){
        for(int j = 0;j<n;j++){
            scanf("%d",&matrix[i][j]);
            if(i >=j ){
                sum += matrix[i][j];
            }
        }
    }
    printf("%d\n",sum);

    
    return 0;
}
